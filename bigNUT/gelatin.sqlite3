/*
  This file implements a gimmick whereby however much gelatin is required to meet the
  glycine dv has its protein added to the protein dv
*/

UPDATE nutr_def
SET    nutopt = 90.0
                + (SELECT nutr_val
                   FROM   nut_in_meals
                   WHERE  ndb_no = 19177
                          AND nutrdesc = 'Protein')
WHERE  nutrdesc = 'Protein';
